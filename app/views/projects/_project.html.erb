<div id="<%= dom_id project %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Title:</strong>
    <%= project.title %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Description:</strong>
    <%= project.description %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Demo video url:</strong>
    <%= project.demo_video_url %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Featured:</strong>
    <%= project.featured? ? "Yes" : "No" %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Role:</strong>
    <%= project.role %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= project.category %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Demo type:</strong>
    <%= project.demo_type %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Slug:</strong>
    <%= project.slug %>
  </div>
</div>
